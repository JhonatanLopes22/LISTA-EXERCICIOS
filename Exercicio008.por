programa {
  funcao inicio() {

    real A, B, C

    escreva("Digite o Valor de A: ")
    leia(A)

    escreva("Digite o Valor de B: ")
    leia(B)

    escreva("Digite o Valor de C: ")
    leia(C)

    se ( A + B < C ){
      escreva(" A Soma de A e B é Menor que C. ")
    }senao{
      escreva(" A Soma de A e B não é Menor que C. ")
    }
  }
}
